{
  "name": "msf-api-primary-device",
  "version": "1.0.0",
  "description": "MSF Backend API",
  "main": "dist/src/index.js",
  "scripts": {
    "start": "node dist/src/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.{ts,js,mjs,cjs}\"",
    "sonar": "ts-node sonar-scanner.ts",
    "test": "vitest run",
    "test:ui": "vitest --ui",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:sonar": "vitest run --coverage && ts-node sonar-scanner.ts",
    "seed": "ts-node src/database/scripts/seed.ts",
    "db:generate": "drizzle-kit generate",
    "db:push": "tsx src/database/scripts/migrate.ts",
    "db:drop": "drizzle-kit drop --config=./drizzle.config.ts",
    "security:audit": "npm audit --production",
    "security:check": "npm ci --audit && npm run security:audit",
    "preinstall": "npm verify-integrity",
    "verify-integrity": "npm audit signatures"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "bullmq": "^5.52.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "drizzle-orm": "^0.43.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.17.0",
    "xlsx": "^0.18.5",
    "xml2js": "^0.6.2",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.27.0",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.18",
    "@types/express": "^5.0.2",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^22.15.19",
    "@types/pg": "^8.15.2",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/xml2js": "^0.4.14",
    "@vitest/coverage-v8": "^3.1.3",
    "@vitest/ui": "^3.1.3",
    "better-sqlite3": "^11.10.0",
    "c8": "^10.1.3",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3",
    "prom-client": "^15.1.3",
    "sonarqube-scanner": "^4.3.0",
    "supertest": "^7.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.1",
    "vitest": "^3.1.3",
    "vitest-mock-extended": "^3.1.0",
    "vitest-sonar-reporter": "^2.0.1"
  },
  "overrides": {
    "esbuild": "^0.25.2"
  },
  "jestSonar": {
    "reportPath": "coverage",
    "reportFile": "test-report.xml",
    "indent": 4
  }
}
